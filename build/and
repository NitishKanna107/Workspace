#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55cabb751e10 .scope module, "and_tb" "and_tb" 2 1;
 .timescale 0 0;
v0x55cabb7769c0_0 .var "a", 0 0;
v0x55cabb776a90_0 .var "b", 0 0;
v0x55cabb776b60_0 .net "y", 0 0, L_0x55cabb776c60;  1 drivers
S_0x55cabb765270 .scope module, "UUT" "and_gate" 2 5, 3 1 0, S_0x55cabb751e10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x55cabb776c60 .functor AND 1, v0x55cabb7769c0_0, v0x55cabb776a90_0, C4<1>, C4<1>;
v0x55cabb7654a0_0 .net "a", 0 0, v0x55cabb7769c0_0;  1 drivers
v0x55cabb7767e0_0 .net "b", 0 0, v0x55cabb776a90_0;  1 drivers
v0x55cabb7768a0_0 .net "y", 0 0, L_0x55cabb776c60;  alias, 1 drivers
    .scope S_0x55cabb751e10;
T_0 ;
    %vpi_call 2 8 "$display", "a | b | y" {0 0 0};
    %vpi_call 2 9 "$monitor", "%b | %b | %b", v0x55cabb7769c0_0, v0x55cabb776a90_0, v0x55cabb776b60_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cabb7769c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cabb776a90_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cabb7769c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cabb776a90_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cabb7769c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cabb776a90_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cabb7769c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cabb776a90_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
